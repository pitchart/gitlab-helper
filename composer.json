{
    "name": "pitchart/gitlab-helper",
    "description": "Commande d'aide pour les apis gitlab",
    "authors": [
        {
            "name": "Julien VITTE",
            "email": "j.vitte@alienor.net"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "pitchart/collection",
                "version": "1.0.0",
                "type": "package",
                "source": {
                    "url": "https://github.com/pitchart/collection.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/console": "^3.1",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/dependency-injection": "^3.1",
        "pitchart/collection": "*",
        "symfony/config": "^3.1",
        "symfony/yaml": "^3.1",
        "padraic/phar-updater": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "pdepend/pdepend": "^2.2",
        "phploc/phploc": "^3.0",
        "phpmetrics/phpmetrics": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Pitchart\\GitlabHelper\\": "src/",
            "Pitchart\\Collection\\": "vendor/pitchart/collection/src/"
        }
    }
}
