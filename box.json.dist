{
    "chmod": "0755",
    "compression": "GZ",
    "finder": [
        {
            "name": "*",
            "exclude": ["Tests"],
            "in": ["src", "config"]
        },
        {
            "name": "*.php",
            "exclude": ["Tests", "Test", "tests", "test"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "bin/gitlab-helper.php",
    "output": "dist/gitlab-helper.phar",
    "stub": true
}